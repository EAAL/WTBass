<!DOCTYPE html>
<html>
  <head>
    <title><%= title%></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <img src="<%= picture.pic%>"/>
    <form action="vote" method="get">
    	<h1>Friends: </h1>
    	<p>
    	<% picture.tags.data.forEach(function(d){ %>
    		<% if(d.id != me) { %>
    		<%= d.name %> 
    		<hr/>
    		<% } %>
    	<% }) %>
    	<%= me %>
    	</p>
    	<input type="submit"/>
    </form>
  </body>
</html>